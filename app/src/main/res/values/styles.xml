<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">#2b4871</item>
        <item name="android:textColorHighlight">#a877d3ef</item>
    </style>
    <style name="actvtheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlNormal">#6c7073</item>
        <item name="colorControlActivated">#334d6b</item>
        <item name="colorControlHighlight">#5266c7</item>
        <item name="android:textColorHighlight">#a877d3ef</item>
    </style>
    <style name="buttheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorControlActivated">#aa334d6b</item>
        <item name="android:background">#c5838181</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
